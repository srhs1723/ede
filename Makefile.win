# GNU ede Makefile for Windows (MinGW)

CC = gcc
CFLAGS = -Wall -Wextra -O2 -std=c11
LDFLAGS = -lkernel32 -luser32
TARGET = ede.exe
SRC = ede.c

.PHONY: all clean

all: $(TARGET)

$(TARGET): $(SRC)
	@echo Building $(TARGET)...
	$(CC) $(CFLAGS) -o $(TARGET) $(SRC) $(LDFLAGS)
	@echo Build complete!

clean:
	@echo Cleaning...
	del /Q $(TARGET) *.o *.emod 2>nul
